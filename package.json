{
  "name": "my-professional-portfolio",
  "version": "1.0.0",
  "description": "this is my angularjs webapp used as my portfolio",
  "main": "app.js",
  "devDependencies": {
    "http-server": "^0.6.1",
    "watchify": "latest",
    "nodemon": "latest"
  },
  "scripts": {
     "prestart": "npm install",
    "build": "npm run less",
    "less": "lessc styles/styles.less > styles/styles.css",
    "watch:less": "nodemon -q -w styles -e less -x npm run less",
    "start": "http-server -a localhost -p 8000 -c-1",
    "watch": "npm run watch:less"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigdragon17/craigdragon17.github.io.git"
  },
